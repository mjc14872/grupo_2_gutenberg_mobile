<!DOCTYPE html>
<html lang="es">

<head>
    <%- include('./partials/head') %>
</head>

<body>
    <header>
        <%- include('./partials/header') %>
    </header>
    <section class="title">
        <h2>LISTADO DE CARRITO</h2>
    </section>
    <section class="product">
        <div class="container">
            <article>
                <% for( let i=0; i < carritos.length; i++ ) { %>
                    <h5>carrito</h5>
                    <h5> <%= carritos[i].titulo %></h5>
                    <h5> <%= carritos[i].editorial %></h5>
                    <h5> <%= carritos[i].cantidad %></h5>
                    <h5> <%= carritos[i].descuento %></h5>
                    <h5> <%= carritos[i].precio %></h5>
                    <h5> <%= carritos[i].total %></h5>
                    <h5> <%= carritos[i].estado %></h5>
                    <% } %>
            </article>
    </section>
    <footer>
        <%- include('./partials/footer') %>
    </footer>
</body>